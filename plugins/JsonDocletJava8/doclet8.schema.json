{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "definitions": {
    "ParsedClassDoc": {
      "type": "object",
      "allOf": [
        { "$ref": "#/definitions/Type" },
        { "$ref": "#/definitions/ProgramElementDoc" },
        {
          "required": [
            "_class",
            "isAbstract",
            "isSerializable",
            "isExternalizable",
            "serializationMethods",
            "serializableFields",
            "definesSerializableFields",
            "interfaceTypes",
            "typeParameters",
            "typeParamTags",
            "fields",
            "enumConstants",
            "methods",
            "constructors",
            "innerClasses",
            "isAnnotationType"
          ],
          "properties": {
            "_class": { "const": "ParsedClassDoc" },
            "isAbstract": { "type": "boolean" },
            "isSerializable": { "type": "boolean" },
            "isExternalizable": { "type": "boolean" },
            "serializationMethods": {
              "type": "array",
              "items": {
                "$ref": "#/definitions/MethodDoc"
              }
            },
            "serializableFields": {
              "type": "array",
              "items": {
                "$ref": "#/definitions/FieldDoc"
              }
            },
            "definesSerializableFields": { "type": "boolean" },
            "superclassType": { "$ref": "#/definitions/AnyType" },
            "interfaceTypes": {
              "type": "array",
              "items": {
                "$ref": "#/definitions/AnyType"
              }
            },
            "typeParameters": {
              "type": "array",
              "items": {
                "$ref": "#/definitions/AnyType"
              }
            },
            "typeParamTags": {
              "type": "array",
              "items": {
                "$ref": "#/definitions/AnyTag"
              }
            },
            "fields": {
              "type": "array",
              "items": {
                "$ref": "#/definitions/FieldDoc"
              }
            },
            "enumConstants": {
              "type": "array",
              "items": {
                "$ref": "#/definitions/FieldDoc"
              }
            },
            "methods": {
              "type": "array",
              "items": {
                "$ref": "#/definitions/MethodDoc"
              }
            },
            "constructors": {
              "type": "array",
              "items": {
                "$ref": "#/definitions/ExecutableMemberDoc"
              }
            },
            "innerClasses": {
              "type": "array",
              "items": {
                "$ref": "#/definitions/ClassDoc"
              }
            },
            "isAnnotationType": { "type": "boolean" },
            "elements": {
              "type": "array",
              "items": {
                "$ref": "#/definitions/AnnotationTypeElementDoc"
              }
            }
          }
        }
      ]
    },
    "ParsedPackageDoc": {
      "type": "object",

      "properties": {
        "allClasses": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/ClassDoc"
          }
        },
        "ordinaryClasses": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/ClassDoc"
          }
        },
        "exceptions": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/ClassDoc"
          }
        },
        "errors": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/ClassDoc"
          }
        },
        "enums": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/ClassDoc"
          }
        },
        "interfaces": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/ClassDoc"
          }
        },
        "annotationTypes": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/ClassDoc"
          }
        },
        "annotations": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/AnnotationDesc"
          }
        }
      }
    },
    "PackageDoc": {
      "type": "object",
      "properties": {
        "_class": { "const": "PackageDoc" },
        "name": { "type": "string" }
      }
    },
    "ClassDoc": {
      "type": "object",
      "allOf": [
        { "$ref": "#/definitions/Type" },
        { "properties": { "_class": { "const": "ClassDoc" } } }
      ]
    },
    "AnyDoc": {
      "additionalProperties": false,
      "oneOf": [
        { "$ref": "#/definitions/ClassDoc" },
        { "$ref": "#/definitions/PackageDoc" },
        { "$ref": "#/definitions/ProgramElementDoc" },
        { "$ref": "#/definitions/MemberDoc" },
        { "$ref": "#/definitions/ExecutableMemberDoc" },
        { "$ref": "#/definitions/MethodDoc" },
        { "$ref": "#/definitions/AnnotationTypeElementDoc" },
        { "$ref": "#/definitions/FieldDoc" }
      ]
    },
    "Doc": {
      "type": "object",
      "properties": {
        "commentText": { "type": "string" },
        "tags": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/Tag"
          }
        },
        "name": { "type": "string" },
        "isField": { "type": "boolean" },
        "isEnumConstant": { "type": "boolean" },
        "isConstructor": { "type": "boolean" },
        "isMethod": { "type": "boolean" },
        "isAnnotationTypeElement": { "type": "boolean" },
        "isInterface": { "type": "boolean" },
        "isException": { "type": "boolean" },
        "isError": { "type": "boolean" },
        "isEnum": { "type": "boolean" },
        "isAnnotationType": { "type": "boolean" },
        "isOrdinaryClass": { "type": "boolean" },
        "isClass": { "type": "boolean" },
        "isIncluded": { "type": "boolean" },
        "position": { "$ref": "#/definitions/SourcePosition" }
      }
    },
    "ProgramElementDoc": {
      "type": "object",
      "allOf": [
        { "$ref": "#/definitions/Doc" },
        {
          "properties": {
            "containingClass": { "$ref": "#/definitions/ClassDoc" },
            "containingPackage": { "$ref": "#/definitions/PackageDoc" },
            "qualifiedName": { "type": "string" },
            "modifierSpecifier": { "type": "number" },
            "modifiers": { "type": "string" },
            "annotations": {
              "type": "array",
              "items": { "$ref": "#/definitions/AnnotationDesc" }
            },
            "isPublic": { "type": "boolean" },
            "isProtected": { "type": "boolean" },
            "isPrivate": { "type": "boolean" },
            "isPackagePrivate": { "type": "boolean" },
            "isStatic": { "type": "boolean" },
            "isFinal": { "type": "boolean" }
          }
        }
      ]
    },
    "MemberDoc": {
      "type": "object",
      "allOf": [
        { "$ref": "#/definitions/ProgramElementDoc" },
        {
          "properties": {
            "isSynthetic": { "type": "boolean" }
          }
        }
      ]
    },
    "ExecutableMemberDoc": {
      "type": "object",
      "allOf": [
        { "$ref": "#/definitions/MemberDoc" },
        {
          "properties": {
            "thrownExceptionTypes": {
              "type": "array",
              "items": { "$ref": "#/definitions/AnyType" }
            },
            "isNative": { "type": "boolean" },
            "isSynchronized": { "type": "boolean" },
            "isVarArgs": { "type": "boolean" },
            "parameters": {
              "type": "array",
              "items": { "$ref": "#/definitions/Parameter" }
            },
            "receiverType": { "$ref": "#/definitions/AnyType" },
            "throwsTags": {
              "type": "array",
              "items": { "$ref": "#/definitions/ThrowsTag" }
            },
            "paramTags": {
              "type": "array",
              "items": { "$ref": "#/definitions/ParamTag" }
            },
            "typeParamTags": {
              "type": "array",
              "items": { "$ref": "#/definitions/ParamTag" }
            },
            "signature": { "type": "string" },
            "flatSignature": { "type": "string" },
            "typeParameters": {
              "type": "array",
              "items": { "$ref": "#/definitions/TypeVariable" }
            },
            "isConstructor": { "type": "boolean" }
          }
        }
      ]
    },
    "MethodDoc": {
      "type": "object",
      "allOf": [
        { "$ref": "#/definitions/ExecutableMemberDoc" },
        {
          "properties": {
            "isAbstract": { "type": "boolean" },
            "isDefault": { "type": "boolean" },
            "returnType": { "$ref": "#/definitions/AnyType" },
            "overriddenType": { "$ref": "#/definitions/AnyType" },
            "overriddenMethodContainingClass": {
              "$ref": "#/definitions/ClassDoc"
            }
          }
        }
      ]
    },
    "AnnotationTypeElementDoc": {
      "type": "object",
      "allOf": [
        { "$ref": "#/definitions/MethodDoc" },
        {
          "properties": {
            "defaultValue": { "type": "string" }
          }
        }
      ]
    },
    "FieldDoc": {
      "type": "object",
      "allOf": [
        { "$ref": "#/definitions/MemberDoc" },
        {
          "properties": {
            "type": { "$ref": "#/definitions/AnyType" },
            "isTransient": { "type": "boolean" },
            "isVolatile": { "type": "boolean" },
            "serialFieldTags": {
              "type": "array",
              "items": { "$ref": "#/definitions/SerialFieldTag" }
            },
            "constantValue": { "type": "string" },
            "constantValueExpression": { "type": "string" }
          }
        }
      ]
    },
    "AnyTag": {
      "oneOf": [
        { "$ref": "#/definitions/ParamTag" },
        { "$ref": "#/definitions/SeeTag" },
        { "$ref": "#/definitions/SerialFieldTag" },
        { "$ref": "#/definitions/ThrowsTag" }
      ]
    },
    "Tag": {
      "type": "object",
      "properties": {
        "name": { "type": "string" },
        "kind": { "type": "string" },
        "text": { "type": "string" },
        "position": { "$ref": "#/definitions/SourcePosition" },
        "inlineTags": {
          "type": "array",
          "items": { "$ref": "#/definitions/Tag" }
        },
        "firstSentenceTags": {
          "type": "array",
          "items": { "$ref": "#/definitions/Tag" }
        }
      }
    },
    "ParamTag": {
      "type": "object",
      "allOf": [
        { "$ref": "#/definitions/Tag" },
        {
          "properties": {
            "_class": { "const": "ParamTag" },
            "parameterName": { "type": "string" },
            "parameterComment": { "type": "string" },
            "isTypeParameter": { "type": "boolean" }
          }
        }
      ]
    },
    "SeeTag": {
      "type": "object",
      "allOf": [
        { "$ref": "#/definitions/Tag" },
        {
          "properties": {
            "_class": { "const": "SeeTag" },
            "label": { "type": "string" },
            "referencedPackage": { "$ref": "#/definitions/PackageDoc" },
            "referencedClassName": { "type": "string" },
            "referencedClass": { "$ref": "#/definitions/ClassDoc" },
            "referencedMemberName": { "type": "string" }
          }
        }
      ]
    },
    "SerialFieldTag": {
      "type": "object",
      "allOf": [
        { "$ref": "#/definitions/Tag" },
        {
          "properties": {
            "_class": { "const": "SerialFieldTag" },
            "fieldName": { "type": "string" },
            "fieldType": { "type": "string" },
            "fieldTypeDoc": { "$ref": "#/definitions/ClassDoc" },
            "description": { "type": "string" }
          }
        }
      ]
    },
    "ThrowsTag": {
      "type": "object",
      "allOf": [
        { "$ref": "#/definitions/Tag" },
        {
          "properties": {
            "_class": { "const": "ThrowsTag" },
            "exceptionName": { "type": "string" },
            "exceptionComment": { "type": "string" },
            "exceptionType": { "$ref": "#/definitions/AnyType" }
          }
        }
      ]
    },
    "AnyType": {
      "type": "object",
      "oneOf": [
        { "$ref": "#/definitions/ParameterizedType" },
        { "$ref": "#/definitions/TypeVariable" },
        { "$ref": "#/definitions/WildcardType" },
        { "$ref": "#/definitions/AnnotatedType" },
        { "$ref": "#/definitions/ClassDoc" },
        { "$ref": "#/definitions/ParsedClassDoc" },
        {
          "allOf": [
            { "$ref": "#/definitions/Type" },
            {
              "properties": { "_class": { "const": "Type" } }
            }
          ]
        }
      ]
    },
    "Type": {
      "type": "object",
      "properties": {
        "_class": {
          "type": "string",
          "enum": [
            "Type",
            "ParameterizedType",
            "TypeVariable",
            "WildcardType",
            "AnnotatedType",
            "ClassDoc",
            "ParsedClassDoc"
          ]
        },
        "asString": { "type": "string" },
        "typeName": { "type": "string" },
        "qualifiedTypeName": { "type": "string" },
        "simpleTypeName": { "type": "string" },
        "dimension": { "type": "string" },
        "isPrimitive": { "type": "boolean" },
        "elementType": { "$ref": "#/definitions/AnyType" },
        "isClass": { "type": "boolean" },
        "isAnnotationType": { "type": "boolean" }
      }
    },
    "ParameterizedType": {
      "type": "object",
      "allOf": [
        { "$ref": "#/definitions/Type" },
        {
          "properties": {
            "_class": { "const": "ParameterizedType" },
            "typeArguments": {
              "type": "array",
              "items": { "$ref": "#/definitions/AnyType" }
            },
            "superclassType": { "$ref": "#/definitions/AnyType" },
            "interfaceTypes": {
              "type": "array",
              "items": { "$ref": "#/definitions/AnyType" }
            },
            "containingType": { "$ref": "#/definitions/AnyType" }
          }
        }
      ]
    },
    "TypeVariable": {
      "type": "object",
      "allOf": [
        { "$ref": "#/definitions/Type" },
        {
          "properties": {
            "_class": { "const": "TypeVariable" },
            "bounds": {
              "type": "array",
              "items": { "$ref": "#/definitions/AnyType" }
            },
            "annotations": {
              "type": "array",
              "items": { "$ref": "#/definitions/AnnotationDesc" }
            }
          }
        }
      ]
    },
    "WildcardType": {
      "type": "object",
      "allOf": [
        { "$ref": "#/definitions/Type" },
        {
          "properties": {
            "_class": { "const": "WildcardType" },
            "extendsBounds": {
              "type": "array",
              "items": { "$ref": "#/definitions/AnyType" }
            },
            "superBounds": {
              "type": "array",
              "items": { "$ref": "#/definitions/AnyType" }
            }
          }
        }
      ]
    },
    "AnnotatedType": {
      "type": "object",
      "allOf": [
        { "$ref": "#/definitions/Type" },
        {
          "properties": {
            "_class": { "const": "AnnotatedType" },
            "annotations": {
              "type": "array",
              "items": { "$ref": "#/definitions/AnnotationDesc" }
            },
            "underlyingType": { "$ref": "#/definitions/AnyType" }
          }
        }
      ]
    },
    "SourcePosition": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "line": { "type": "number" },
        "column": { "type": "number" },
        "file": { "type": "string" }
      }
    },
    "AnnotationValue": {
      "type": "string"
    },
    "AnnotationDesc": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "annotationType": { "$ref": "#/definitions/ClassDoc" },
        "elementValues": {
          "type": "array",
          "items": {
            "type": "object",

            "properties": {
              "element": { "$ref": "#/definitions/ClassDoc" },
              "value": { "$ref": "#/definitions/AnnotationValue" }
            }
          }
        },
        "isSynthesized": { "type": "boolean" }
      }
    },
    "Parameter": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "type": { "$ref": "#/definitions/AnyType" },
        "name": { "type": "string" },
        "typeName": { "type": "string" },
        "asString": { "type": "string" },
        "annotations": {
          "type": "array",
          "items": { "$ref": "#/definitions/AnnotationDesc" }
        }
      }
    }
  },
  "oneOf": [{ "$ref": "#/definitions/ParsedClassDoc" }]
}
